package com.ilpexperion.service;

import com.ilpexperion.entity.Card;

public class Service {
	public static Product createProduct(String productType) {
		// TODO Auto-generated method stub
		
		Product product = null;
		if(productType.compareTo("Cards")==0)
		{
			Product product = new Card("PRDMC100","MasterCard","Cards","Master");
			
		}
		else  if(productType.compareTo("Account")==0)
		{
			Product product = new Card("PRDWSA100","Womens Savings Account","Accounts","Savings");
			
		}
		return product;
		
		
		
	}



	public static void buyProduct() {
		// TODO Auto-generated method stub
		
	}

}
